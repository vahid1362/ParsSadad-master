@model  QtasHelpDesk.ViewModels.Content.GroupViewModel

@{
    ViewData["Title"] = "ایجاد گروه ها";

    Layout = "~/Views/Shared/_AdminLayOut.cshtml";
}
<link href="~/Lib/Select2/css/select2.min.css" rel="stylesheet" />
@using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "form-horizontal" }))
{
    <section class="content">
        <div class="box box-info">
            <div class="box-header with-border">
                <div class="row">
                    <div class="col-md-6">
                        <h3 class="box-title">ایجاد گروه</h3>
                    </div>
                    <div class="text-left">
                        <button type="submit" class="btn btn-success"><i class="fa fa-save fa-lg"></i>ذخیره</button>

                        <a href="@Url.Action("List")" class="btn btn-default"><i class="fa fa-backward fa-lg"></i>بازگشت</a>
                    </div>
                </div>
            </div>
            @Html.Partial("_CreateOrUpdate",@Model)
        </div>

    </section>

}
@section scripts
{
    <script src="~/Lib/Select2/js/select2.js"></script>
    <script>


        $('#DropDown').select2({ dir: "rtl" });
    </script>

}

