@model IEnumerable<QtasHelpDesk.ViewModels.Content.FaqViewModel>

@foreach (var faqwModel in Model)
    {

       <div class="card">
            <div class="card-header" style="">
                <h6 class="panel-title " itemprop="headline_name">
                    <i class="fa fa-question-circle"></i>
                    @faqwModel.Question
                </h6>
            </div>
               <div class="card-body"><i class="fa fa-reply"></i> پاسخ: @faqwModel.Reply</div>
            <div class="card-footer">  <i class="fa fa-user-circle"></i> نوشته شده توسط :@faqwModel.UserFullName<i class="fa fa-clock-o"></i>  @faqwModel.Date</div>

                </div>
        <br />

    }