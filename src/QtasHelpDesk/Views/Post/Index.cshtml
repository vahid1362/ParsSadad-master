@model IEnumerable<QtasHelpDesk.ViewModels.Content.PostViewModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@foreach (var postviewModel in Model)
{
    <div class="card" >
        <div class="card-header bg-info" style="">
            <h6 class="panel-title text-white" itemprop="headline_name">
                
                @postviewModel.Title
            </h6>
        </div>
        <div class="card-body"> @postviewModel.Summary</div>
       
    </div>
    <br/>
}



