@model IEnumerable<QtasHelpDesk.ViewModels.Content.GroupViewModel>

<div class="card">
    <div class="card-header">
        <span class="fa fa-tag" aria-hidden="true"></span>
        گروه‌ها
    </div>
    <div class="card-body">
        <ul dir="ltr" class="list-group">
           @foreach (var group in Model)
            {
                <li class="list-group-item" role="presentation" dir="ltr">
                  
                    <span > @group.Title</span>
                </li>
            }

        </ul>
    </div> 
    <div class="card-footer"></div>

</div>